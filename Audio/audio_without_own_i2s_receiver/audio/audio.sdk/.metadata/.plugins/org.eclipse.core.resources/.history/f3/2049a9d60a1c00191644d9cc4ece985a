#include "push_buttons.h"
#include "stdio.h"

static volatile unsigned int *const push_buttons = (unsigned int*) (GPIO_BASE_ADDRESS);

int main(){
	while(1){
		unsigned int a = *push_buttons;
		switch(a) {
		case push_button_up:
			printf("up\n");
			break;
		case push_button_down:
			printf("down\n");
			break;
		}
	}
}


