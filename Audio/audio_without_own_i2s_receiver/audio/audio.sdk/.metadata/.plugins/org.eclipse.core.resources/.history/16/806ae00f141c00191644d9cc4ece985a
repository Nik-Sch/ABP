#include "push_buttons.h"
#include "stdio.h"

void checkout_push_buttons(){
	unsigned int a,b = 0;
	while(1){

		unsigned int a = *push_buttons;
		if (a == push_button_up && a != b){
			printf("up\n");
			b = a;
			while(b == a) {
				a = *push_buttons;
			}
		}
	}
}

